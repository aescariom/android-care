<?xml version="1.0" encoding="utf-8"?> 

<datastore-indexes  autoGenerate="false">

    <!-- Used 1 time in query history -->
    <datastore-index kind="ReminderLog" ancestor="true" source="auto">
        <property name="time" direction="desc"/>
    </datastore-index>

    <!-- Used 1 time in query history -->
    <datastore-index kind="Reminder" ancestor="false" source="auto">
        <property name="owner" direction="asc"/>
        <property name="title" direction="asc"/>
    </datastore-index>

    <!-- Used 1 time in query history -->
    <datastore-index kind="Position" ancestor="false" source="auto">
        <property name="owner" direction="asc"/>
        <property name="date" direction="desc"/>
    </datastore-index>

    <datastore-index kind="Alarm" ancestor="false" source="auto">
        <property name="owner" direction="asc"/>
    </datastore-index>

</datastore-indexes>
