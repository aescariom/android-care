<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='dashboard'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='net.sf.gilead.adapter4appengine.Adapter4AppEngine'/>

    <!-- Other module inherits                                      -->
    <!--  Google maps -->
    <inherits name='com.google.gwt.maps.GoogleMaps' />

    <!-- Specify the app entry point class.                         -->
    <entry-point class='org.androidcare.web.client.module.dashboard.Dashboard'/>

    <!-- Specify the Location -->
    <extend-property name="locale" values="es"/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client/module/dashboard'/>
    <source path='client/observer'/>
    <source path='client/widgets'/>
    <source path='client/internacionalization'/>
    <source path='shared'/>
</module>